type GoogleInfo {
  etag: String
  familyName: String
  givenName: String
  kind: String
  objectType: String
  url: String
  language: String
  isPlusUser: Boolean
  verified: Boolean
}

type User {
  id: ID! @unique
  providerId: String! @unique
  googleInfo: GoogleInfo!
  displayName: String!
  picture: String!
  refreshToken: String!
}
